{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "resolveJsonModule": true,
  },
  "compileOnSave": true,
  "include": [
    "src/**/*",
    "development/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "build",
    "src/**/__tests__/**/*",
    "development/**/__tests__/**/*"
  ]
}